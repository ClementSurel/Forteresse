                                                          wgetparent               ooppaaqquuee(3NCURSES)*
                                                          wgetscrreg               ooppaaqquuee(3NCURSES)*
                                                          wgetstr                  ggeettssttrr(3NCURSES)
                                                          whline                   bboorrddeerr(3NCURSES)
                                                          whline_set               bboorrddeerr__sseett(3NCURSES)
                                                          win_wch                  iinn__wwcchh(3NCURSES)
                                                          win_wchnstr              iinn__wwcchhssttrr(3NCURSES)
                                                          win_wchstr               iinn__wwcchhssttrr(3NCURSES)
                                                          winch                    iinncchh(3NCURSES)
                                                          winchnstr                iinncchhssttrr(3NCURSES)
