                                                          wattron                  aattttrr(3NCURSES)
                                                          wattrset                 aattttrr(3NCURSES)
                                                          wbkgd                    bbkkggdd(3NCURSES)
                                                          wbkgdset                 bbkkggdd(3NCURSES)
                                                          wbkgrnd                  bbkkggrrnndd(3NCURSES)
                                                          wbkgrndset               bbkkggrrnndd(3NCURSES)
                                                          wborder                  bboorrddeerr(3NCURSES)
                                                          wborder_set              bboorrddeerr__sseett(3NCURSES)
                                                          wchgat                   aattttrr(3NCURSES)
                                                          wclear                   cclleeaarr(3NCURSES)
                                                          wclrtobot                cclleeaarr(3NCURSES)
                                                          wclrtoeol                cclleeaarr(3NCURSES)
                                                          wcolor_set               aattttrr(3NCURSES)
                                                          wcursyncup               wwiinnddooww(3NCURSES)
                                                          wdelch                   ddeellcchh(3NCURSES)
                                                          wdeleteln                ddeelleetteellnn(3NCURSES)
                                                          wecho_wchar              aadddd__wwcchh(3NCURSES)
                                                          wechochar                aaddddcchh(3NCURSES)
                                                          wenclose                 mmoouussee(3NCURSES)*
                                                          werase                   cclleeaarr(3NCURSES)
                                                          wget_wch                 ggeett__wwcchh(3NCURSES)
                                                          wget_wstr                ggeett__wwssttrr(3NCURSES)
